{
    "name": "Elve Desktop",
    "main": "top.html",
    "chromium-args": "--disable-gpu --force-cpu-draw --enable-transparent-visuals --ozone-platform=wayland",
    "user-agent":"Mozilla/5.0 (Linux; Android 10; K) AppleWebKit/537.36 (KHTML, like Gecko) Chrome/114.0.0.0 Mobile Safari/537.36",
    "window": {
        "frame": true,
        "position": "center",
        "icon":"",
        "transparent":true,
        "width": 1920,
        "height": 1080,
        "resizable": false,
        "always-on-top":true,
        "fullscreen":true
     },
     "webview": { 
        "partitions": [
           {
             "name": "trusted",
             "accessible_resources": [
                "<all_urls>"
             ] 
           }
        ]
      }
     
  }
